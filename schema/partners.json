{
  "id": "partners",
  "type": "Collection",
  "info": {
    "title": "Partners",
    "singularName": "partner",
    "pluralName": "partners",
    "description": "Partners with unique IDNO and Legal Entity"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": [
    {
      "id": "idno",
      "type": "Text",
      "unique": true,
      "required": true,
      "constraints": {
        "pattern": "^[0-9]{13}$"
      }
    },
    {
      "id": "legal_entity",
      "type": "Text",
      "unique": true,
      "required": true
    }
  ],
  "relations": [
    {
      "id": "brands",
      "relation": "HasMany",
      "target": "brands"
    },
    {
      "id": "category",
      "relation": "HasOne",
      "target": "partner-categories"
    },
    {
      "id": "points-of-sale",
      "relation": "HasMany",
      "target": "points-of-sale"
    }
  ]
}