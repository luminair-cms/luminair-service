        "maximalLength": 10
      }
    },
    {
      "id": "name",
      "type": "Text",
      "required": true,
      "localized": true
    }
  ],
  "relations": [
    {
      "id": "partners",
      "relation": "BelongsToMany",
      "target": "partners"
    }
  ]
