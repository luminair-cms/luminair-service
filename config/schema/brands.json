{
  "type": "Collection",
  "info": {
    "title": "Brands",
    "singularName": "brand",
    "pluralName": "brands",
    "description": "Brands of partners"
  },
  "options": {
    "draftAndPublish": true,
    "localizations": ["en", "ro"]
  },
  "attributes": {
    "uid": {
      "type": "Uid",
      "unique": true,
      "required": true,
      "constraints": {
        "minimalLength": 4,
        "maximalLength": 10
      }
    },
    "name": {
      "type": "Text",
      "required": true,
      "localized": true
    },
    "partners": {
      "relation": "BelongsToMany",
      "target": "partners"
    }
  }
}
