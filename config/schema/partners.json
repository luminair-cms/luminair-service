{
  "type": "Collection",
  "info": {
    "title": "Partners",
    "singularName": "partner",
    "pluralName": "partners",
    "description": "Partners with unique IDNO and Legal Entity"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "idno": {
      "type": "Text",
      "unique": true,
      "required": true,
      "constraints": {
        "pattern": "^[0-9]{13}$"
      }
    },
    "legal_entity": {
      "type": "Text",
      "unique": true,
      "required": true
    },
    "brands": {
      "relation": "HasMany",
      "target": "brands"
    },
    "category": {
      "relation": "HasOne",
      "target": "partner-categories"
    },
    "points-of-sale": {
      "relation": "HasMany",
      "target": "points-of-sale"
    }
  }
}